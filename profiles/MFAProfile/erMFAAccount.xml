<page>
	<body>
		<tabbedForm>
			<tab index="0" selected="true">
				<title>$tag.as.User</title>
				<image/>
				<url>javascript:switchTabs(document.forms['body'],0);</url>
				<formElement label="$eruid" name="data.eruid" required="true">
					<input name="data.eruid" size="50" type="text"/>
					<constraint>
						<type>REQUIRED</type>
						<parameter>true</parameter>
					</constraint>
				</formElement>				
				<formElement label="$eremail" name="data.eremail">
					<input name="data.eremail" size="50" type="text"/>
				</formElement>
				<formElement label="$erphoneNumber" name="data.erphoneNumber">
					<input name="data.erphoneNumber" size="50" type="text"/>
				</formElement>
				<formElement label="$ersmsstate" name="data.ersmsstate">
					<input name="data.ersmsstate" size="50" type="text"/>
				</formElement>
				<formElement direction="inherit" label="$erroleId" name="data.erroleId">
		            <searchFilter multiple="true" type="select">
		               <filter>(objectclass=erMFAMethods)</filter>
		               <base>contextual</base>
		               <attribute>erroleId</attribute>
		               <sourceAttribute>erroleId</sourceAttribute>
		               <delimiter></delimiter>
		               <size></size>
		               <width>300</width>
		               <objectClass></objectClass>            
		               <showQueryUI>false</showQueryUI>
		               <paginateResults>false</paginateResults>
		            </searchFilter>
		         </formElement>
			</tab>
		</tabbedForm>
	</body>
</page>