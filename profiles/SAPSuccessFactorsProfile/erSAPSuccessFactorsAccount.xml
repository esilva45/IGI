				<formElement label="MATRICULA" name="data.erpersonnelnumber">
					<input name="data.erpersonnelnumber" size="50" type="erpersonnelnumber"/>
				</formElement>
				<formElement label="USER ID" name="data.eruserid">
					<input name="data.eruserid" size="50" type="eruserid"/>
